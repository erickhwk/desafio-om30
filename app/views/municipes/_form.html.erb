<%= simple_form_for @municipe do |f| %>
  <%= f.input :name %>
  <%= f.input :cpf %>
  <%= f.input :cns %>
  <%= f.input :email %>
  <%= f.input :birth_date, as: :date, html5: true %>
  <%= f.input :phone %>
  <%= image_tag "#{@municipe.photo_url(:thumb)}" %>
  <%= f.file_field :photo %>
  <%= f.input :status, collection: ['active', 'inactive'], selected: 'active'  %>
  <%= f.button :submit %>
<% end %>
